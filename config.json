{
    "name": "OpenVPN Client",
    "version": "0.2.3",
    "slug": "openvpn-client",
    "description": "Connect Hassio to a remote VPN Server",
    "startup": "application",
    "boot": "auto",
    "host_network": true,
    "privileged": ["NET_ADMIN"],
    "options": {
      "config" : [
        "client",
        "ca ca.crt",
        "cert client.crt",
        "key client.key",
        "dev tun",
        "proto tcp",
        "resolv-retry infinite",
        "nobind",
        "persist-key",
        "persist-tun",
        "remote-cert-tls server",
        "tls-auth ta.key 1",
        "cipher AES-256-CBC",
        "comp-lzo",
        "remote <SERVER ADRESS> <PORT>"
        ]
    },
    "schema": {
      "config" : ["str"]
    },
    "webui": "http://[HOST]:[PORT:5000]",
    "ports": {
      "5000/tcp" : 5000
    },
    "map" : ["share:rw"]
  }
